!-include head-!
<div class='box'>
  <div class='title'>System log</div>
  !-if not logs-!
  <div class='information'>No log to show</div>
  !-else-!
  <table class='makeup-table'>
    !-foreach logs as log-!
    <tr>
      <td>[!-log["time"]-!]!-log["message"]-!</td>
    </tr>
    !-endblock-!
  </table>
  !-endblock-!
</div>
!-include buttom-!