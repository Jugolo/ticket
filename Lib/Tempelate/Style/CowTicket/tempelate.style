!-addCss style.table-!
!-include head-!
<div class='box'>
  <div class='title'>Tempelates</div>
  <table class='makeup-table'>
    !-foreach tempelates as tempelate-!
    <tr>
      <th>!-tempelate["name"]-!</th>
      !-if tempelate["is_current"]-!
      <td>Is current tempelate</td>
      !-else-!
      <td><a href='?view=tempelate&select=!-tempelate["name"]-!'>Select</a></td>
      !-endblock-!
    </tr>
    !-endblock-!
  </table>
</div>
!-include buttom-!