!-addCss style.error-!
!-addCss style.table-!
!-include head-!
<div class='box'>
  <div class='title'>Error detail</div>
  <table id='error-detail'>
    <tr>
      <th>File</th>
      <td>!-file-!</td>
    </tr>
    <tr>
      <th>Line</th>
      <td>!-line-!</td>
    </tr>
    <tr>
      <th>Time</th>
      <td>!-time-!</td>
    </tr>
    <tr>
      <th colspan="2">Message</th>
    </tr>
    <tr>
      <td colspan="2">!-message-!</td>
    </tr>
  </table>
</div>

<div class="box">
  <div class='title'>Error file</div>
  <table id="error-file">
    !-foreach lines as data-!
    <tr class="!-if data['number'] == line-!current!-else-!not!-endblock-!">
      <th>!-data["number"]-!</th>
      <td>!-data["line"]-!</td>
    </tr>
    !-endblock-!
  </table>
</div>

!-if other_error-!
<div class="box">
  <div class="title">Same error</div>
  <table class="makeup-table">
    !-foreach other_error as error-!
    <tr>
      <th><a href="?view=error&id=!-error['id']-!">!-error["errstr"]-!</a></th>
    </tr>
    !-endblock-!
  </table>
</div>
!-endblock-!
!-include buttom-!